{
    "compilerOptions": {
        "outDir": "src/js",
        "target": "ES6",
        "noEmitOnError": true,
        "noImplicitAny": true,
        "removeComments": true,
        "strictNullChecks": true,
        "experimentalDecorators": true,
        "moduleResolution": "node",
        "baseUrl": ".",
        "paths": {
            "@axios/*": [
                "node_modules/axios/*"
            ]
        }
        // "sourceMap": true
    },
    "include": ["src/**/*"]
}